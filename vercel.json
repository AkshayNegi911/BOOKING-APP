{
    "buildCommand": "(cd Client) && (npm i) && (./node_modules/vite/bin/vite.js build)",
    "outputDirectory": "Client/dist",
    "framework": "vite",
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/index.js"
      },
      {"source": "/(.*)", "destination": "/"}
    ]
  }